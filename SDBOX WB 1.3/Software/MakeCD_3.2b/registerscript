.KEY language/A
.BRA {
.KET }

; $Id: registerscript 1.7 1998/08/13 08:25:05 angela Exp $

; If you want to add support for further languages, please
; contact angela@core.de first to check if somebody else is already
; working on such a translation.

SET makecdlang {language}

; By Angela Schmidt
; *** This is the filename under which the registration form is saved
SET makecdregfilename "Registrationform"
SET makecdnoregform "Sorry, could not find registration form for MakeCD."
SET makecdregreq "Registration Request"
SET makecdshallprint "Do you want the registration form being printed now?"
SET makecdshalldelete "Do you want the registration form in T: being deleted now?"
SET makecdansok "OK"
SET makecdansyes "Yes"
SET makecdansno "No"

; by Angela Schmidt
IF $makecdlang EQ "deutsch"
  SET makecdregfilename "Registrierformular"
  SET makecdnoregform "Leider konnte kein Registrierformular für MakeCD gefunden werden."
  SET makecdregreq "Registrations-Dialogfenster"
  SET makecdshallprint "Soll das Registrierformular nun gedruckt werden?"
  SET makecdshalldelete "Soll das Registrierformular nun aus T: gelöscht werden?"
  SET makecdansok "OK"
  SET makecdansyes "Ja"
  SET makecdansno "Nein"
ENDIF

; By Mark Holm <mholm@email.dk>
IF $makecdlang EQ "dansk"
  SET makecdregfilename "Registreringsformular"
  SET makecdnoregform "Beklager, kan ikke finde MakeCD's registreringsformular."
  SET makecdregreq "Registreringsforespørgsel"
  SET makecdshallprint "Vil du udskrive registreringsformularen nu?"
  SET makecdshalldelete "Vil du slette registreringsformularen i T: nu?"
  SET makecdansok "OK"
  SET makecdansyes "Ja"
  SET makecdansno "Nej"
ENDIF


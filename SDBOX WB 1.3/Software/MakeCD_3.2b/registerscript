  SET makecdregreq "Regisztrációs kérés"
  SET makecdshallprint "Kívánja a regisztrációs ûrapot most kinyomtatni?"
  SET makecdshalldelete "Kívánja a T:-ben lévõ regisztrációs ûrlapot most letörölni?"
  SET makecdansok "OK"
  SET makecdansyes "Igen"
  SET makecdansno "Nem"

  SET makecdregfilename "FormEnregistrement"
  SET makecdnoregform "Désolé, impossible de trouver le formulaire d'enregistrement pour MakeCD."
  SET makecdregreq "Requête d'enregistrement"
  SET makecdshallprint "Voulez-vous que la fiche d'enregistrement soit imprimée maintenant ?"
  SET makecdshalldelete "Voulez-vous que la fiche d'enregistrement dans T: soit effacée maintenant ?"
  SET makecdansok "OK"
  SET makecdansyes "Oui"
  SET makecdansno "Non"
ENDIF


; By Mikko Virtanen <mtv@klinja.fipnet.fi>
IF $makecdlang EQ "suomi"
  SET makecdregfilename "Rekisteröintikaavake"
  SET makecdnoregform "En löytänyt rekisteröintikaavaketta MakeCD:lle."
  SET makecdregreq "Rekisteröintipyyntö"
  SET makecdshallprint "Haluatko, että rekisteröintikaavake tulostetaan nyt?"
  SET makecdshalldelete "Haluatko, että T:ssä oleva rekisteröintikaavake tuhotaan nyt?"
  SET makecdansok "OK"
  SET makecdansyes "Kyllä"
  SET makecdansno "Ei"
ENDIF

; By Fredrik Zetterlund <frallan@canit.se>
IF $makecdlang EQ "svenska"
  SET makecdregfilename "Registreringsformulär"
  SET makecdnoregform "Hittar inte registreringsformuläret för MakeCD."
  SET makecdregreq "Registreringsförfrågan"
  SET makecdshallprint "Vill du skriva ut registreringsformuläret nu?"
  SET makecdshalldelete "Vill du radera registreringsformuläret i T: nu?"
  SET makecdansok "Okej"
  SET makecdansyes "Ja"
  SET makecdansno "Nej"
ENDIF

IF NOT EXISTS doc/$makecdlang/$makecdregfilename
  SET makecdlang english
  SET makecdregfilename "Registrationform"
ENDIF

IF NOT EXISTS doc/$makecdlang/$makecdregfilename
  SET makecdlang deutsch
  SET makecdregfilename "Registrierformular"
ENDIF

IF EXISTS doc/$makecdlang/$makecdregfilename
  Copy doc/$makecdlang/$makecdregfilename T:Registrationform
  Protect T:Registrationform rwd
  IF EXISTS ENV:EDITOR
    $EDITOR T:Registrationform
  ELSE
    Ed T:Registrationform
  ENDIF
ELSE
  IF EXISTS C:RequestChoice
    RequestChoice >NIL: "$makecdregreq" "$makecdnoregform" "$makecdansok"
  ELSE
    Echo >CON:0/0/640/100/Error "$makecdnoregform"
  ENDIF
  SKIP unsetvars
ENDIF

IF EXISTS C:RequestChoice
  RequestChoice >ENV:makecdANSWER "$makecdregreq" "$makecdshallprint" "$makecdansyes" "$makecdansno"
  IF $makecdANSWER EQ 1
    Type T:Registrationform TO PRT:
  ENDIF
  RequestChoice >ENV:makecdANSWER "$makecdregreq" "$makecdshalldelete" "$makecdansyes" "$makecdansno"
  IF $makecdANSWER EQ 1
    Delete QUIET T:Registrationform
  ENDIF
  Delete QUIET ENV:makecdANSWER
ENDIF

LAB unsetvars

UNSET makecdlang
UNSET makecdregfilename
UNSET makecdnoregform
UNSET makecdregreq
UNSET makecdshallprint
UNSET makecdshalldelete
UNSET makecdansok
UNSET makecdansyes
UNSET makecdansno

LAB end

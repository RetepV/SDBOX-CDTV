			"Deleting old files and directories."
		)
		(set #no_catalog
			"no catalog"
		)
		(set #catalog
			"catalog"
		)
		(set #no_doc
			"no docs"
		)
		(set #doc
			"docs"
		)
		(set #cannot_find_lang_docs
			("%s%s%s%s"                         ; *** CHANGED ***
				"Sorry, cannot find any docs for language \"%s\".\n\n"
				"Installation of the docs of this language skipped.\n\n"

; If you want to add support for further languages, please
; contact angela@core.de first to check if somebody else is
; already working on such a translation.

(procedure P_GET_LANG_FROM_ENV
	(
		(if (= 1 (exists "ENV:Language"))
			( ; then
				(if (patmatch (getenv "language") txt_lang_0) ; Dansk
					0
					(if (patmatch (getenv "language") txt_lang_1) ; Deutsch
						1
						(if (patmatch (getenv "language") txt_lang_2) ; English
							2
							(if (patmatch (getenv "language") txt_lang_3) ; Español
								3
								(if (patmatch (getenv "language") txt_lang_4) ; Français
									4
									(if (patmatch (getenv "language") txt_lang_5) ; Italiano
										5
										(if (patmatch (getenv "language") txt_lang_6) ; Magyar
											6
											(if (patmatch (getenv "language") txt_lang_7) ; Nederlands
												7
												(if (patmatch (getenv "language") txt_lang_8) ; Norsk
													8
													(if (patmatch (getenv "language") txt_lang_9) ; Suomi
														9
														(if (patmatch (getenv "language") txt_lang_10) ; Svenska
															10
															(if (patmatch (getenv "language") txt_lang_11) ; ÃeÓtina (Czech)
																11
																2 ; English
															)
														)
													)
												)
											)
										)
									)
								)
							)
						)
					)
				)
			)
			( ; else
				(if (patmatch @language txt_lang_0) ; Dansk
					0
					(if (patmatch @language txt_lang_1) ; Deutsch
						1
						(if (patmatch @language txt_lang_2) ; English
							2
							(if (patmatch @language txt_lang_3) ; Español
								3
								(if (patmatch @language txt_lang_4) ; Français
									4
									(if (patmatch @language txt_lang_5) ; Italiano
										5
										(if (patmatch @language txt_lang_6) ; Magyar
											6
											(if (patmatch @language txt_lang_7) ; Nederlands
												7
												(if (patmatch @language txt_lang_8) ; Norsk
													8
													(if (patmatch @language txt_lang_9) ; Suomi
														9
														(if (patmatch @language txt_lang_10) ; Svenska
															10
															(if (patmatch @language txt_lang_10) ; ÃeÓtina (Czech)
																11
																2 ; English
															)
														)
													)
												)
											)
										)
									)
								)
							)
						)
					)
				)
			)
		)
	)
)



; Für neue Sprache (Catalog oder Anleitung für MakeCD):
; - Sprache hier einfügen
; - numlangs auf Anzahl der von MakeCD supporteten Sprachen setzen
; - bei #which_languages_to_install Sprache hinzufügen

(procedure P_SELECT_LANGUAGE
	(
;                                   |                                  | 2 if doc exists,         |                              | 
;     Name of language              | Name of Catalog directory        | 1 if catalog exists,     | Translation of "by"          | Name of translator
;                                   |                                  | 0 if xyz does not exist  |                              |
;     ==============================|==================================|==========================|==============================|=============================================
		(set txt_lang_0  "Dansk")        (set txt_langdir_0  "dansk")        (set langbits_0  (+ 0 1))   (set txt_by_0  "af")            (set trans_lang_0  "Mark Holm")
		(set txt_lang_1  "Deutsch")      (set txt_langdir_1  "deutsch")      (set langbits_1  (+ 2 1))   (set txt_by_1  "von")           (set trans_lang_1  "Angela Schmidt")
		(set txt_lang_2  "English")      (set txt_langdir_2  "english")      (set langbits_2  (+ 2 0))   (set txt_by_2  "by")            (set trans_lang_2  "Angela Schmidt")
		(set txt_lang_3  "Español")      (set txt_langdir_3  "español")      (set langbits_3  (+ 0 1))   (set txt_by_3  "por")           (set trans_lang_3  "Javier Morata")
		(set txt_lang_4  "Français")     (set txt_langdir_4  "français")     (set langbits_4  (+ 0 1))   (set txt_by_4  "par")           (set trans_lang_4  "Jean-Marc Boursot")
		(set txt_lang_5  "Italiano")     (set txt_langdir_5  "italiano")     (set langbits_5  (+ 0 1))   (set txt_by_5  "a cura di")     (set trans_lang_5  "Francesco Dipietromaria")
		(set txt_lang_6  "Magyar")       (set txt_langdir_6  "magyar")       (set langbits_6  (+ 0 1))   (set txt_by_6  "készítette")    (set trans_lang_6  "Horváth Péter")
		(set txt_lang_7  "Nederlands")   (set txt_langdir_7  "nederlands")   (set langbits_7  (+ 0 1))   (set txt_by_7  "door")          (set trans_lang_7  "Tim J. en Korneel K.")          ; (Tim Jacobs en Korneel Ketelslegers)
		(set txt_lang_8  "Norsk")        (set txt_langdir_8  "norsk")        (set langbits_8  (+ 0 1))   (set txt_by_8  "av")            (set trans_lang_8  "Eivind Olsen")
		(set txt_lang_9  "Suomi")        (set txt_langdir_9  "suomi")        (set langbits_9  (+ 0 1))   (set txt_by_9  "tehnyt")        (set trans_lang_9  "Mikko Virtanen")
		(set txt_lang_10 "Svenska")      (set txt_langdir_10 "svenska")      (set langbits_10 (+ 0 1))   (set txt_by_10 "av")            (set trans_lang_10 "Fredrik Zetterlund")
		(set txt_lang_11 "ÃeÓtina")      (set txt_langdir_11 "ÃeÓtina")      (set langbits_11 (+ 0 1))   (set txt_by_10 "pÒeloÚil")      (set trans_lang_10 "VÉt óindlÁÒ")
;		The following languages are not available in this installer script, but there might be a catalog etc.
		(set txt_lang_12 "Greek")        (set txt_langdir_12 "greek")        (set langbits_12 (+ 0 1))
		; looking for português, ...

		(set numlangs 13)

		(if (AND (= 0 origuserlevel) (= 1 (exists "ENV:Language")))
			(
				(set langnum (P_GET_LANG_FROM_ENV))
			)
			(
				(user 2)
				(set langnum
					(askchoice
						(prompt "Choose the language to be used in this installer script.")
						(help "Please select your favourite language.\n\n" "Bitte wählen Sie ihre bevorzugte Sprache.\n\n"
							("%s: %s %s" txt_lang_0 txt_by_0 trans_lang_0)
							("%s: %s %s" txt_lang_1 txt_by_1 trans_lang_1)
							("%s: %s %s" txt_lang_2 txt_by_2 trans_lang_2)
							("%s: %s %s" txt_lang_3 txt_by_3 trans_lang_3)
							("%s: %s %s" txt_lang_4 txt_by_4 trans_lang_4)
							("%s: %s %s" txt_lang_5 txt_by_5 trans_lang_5)
							("%s: %s %s" txt_lang_6 txt_by_6 trans_lang_6)
							("%s: %s %s" txt_lang_7 txt_by_7 trans_lang_7)
							("%s: %s %s" txt_lang_8 txt_by_8 trans_lang_8)
							("%s: %s %s" txt_lang_9 txt_by_9 trans_lang_9)
							("%s: %s %s" txt_lang_10 txt_by_10 trans_lang_10)
							("%s: %s %s" txt_lang_10 txt_by_10 trans_lang_11)
						)
						(choices
							("%s" txt_lang_0)
							("%s" txt_lang_1)
							("%s" txt_lang_2)
							("%s" txt_lang_3)
							("%s" txt_lang_4)
							("%s" txt_lang_5)
							("%s" txt_lang_6)
							("%s" txt_lang_7)
							("%s" txt_lang_8)
							("%s" txt_lang_9)
							("%s" txt_lang_10)
							("%s" txt_lang_11)
						)
						(default
							(P_GET_LANG_FROM_ENV)
						)
					)
				)
				(user origuserlevel)
			)
		)

		(if (= langnum 0)  (set langstring txt_lang_0))
		(if (= langnum 1)  (set langstring txt_lang_1))
		(if (= langnum 2)  (set langstring txt_lang_2))
		(if (= langnum 3)  (set langstring txt_lang_3))
		(if (= langnum 4)  (set langstring txt_lang_4))
		(if (= langnum 5)  (set langstring txt_lang_5))
		(if (= langnum 6)  (set langstring txt_lang_6))
		(if (= langnum 7)  (set langstring txt_lang_7))
		(if (= langnum 8)  (set langstring txt_lang_8))
		(if (= langnum 9)  (set langstring txt_lang_9))
		(if (= langnum 10) (set langstring txt_lang_10))
		(if (= langnum 11) (set langstring txt_lang_11))
		(if (= langnum 12) (set langstring txt_lang_12))
		(if (= langnum 13) (set langstring txt_lang_13))

		(if (<> 1 (exists "ENV:Language"))
			(
				(run ("%s%s" "setenv Language " langstring))
			)
		)


		; Set default strings

		; By Angela Schmidt
